        
            <header class="header">
                <div >
                    {{> logo}}
                </div>

                <nav class="header_nav ">
                    <div class="header_nav_right">
                        {{> nav list=header}}
                        </div>
                </nav>
                    </header>


        
        <main class="main">
            
            {{#with info}}
            <div class="row">
            <h2 class="info_title">{{title}}</h2>
            <p class="info_description">{{description}}</p>
               </div>
            {{/with}}

        

       
        {{#with form}}
        <form class="form">
            <h2 class="form_title">{{title}}</h2>

            <div class="form_input_list">
                {{#each inputs}}
                <input type="{{type}}" placeholder="{{placeholder}} " class="form_input" name="name" 
                id="form-input-{{id}}" />
                {{/each}}
            </div>
            <div class="form_checkbox">
                <input type="checkbox" name="{{checkbox.name}}" id="{{checkbox.name}}">
                <div></div>
                <label for="{{checkbox.name}}"> {{checkbox.label}}"</label>

            </div>
            <button type="submit" class="form_button">{{button}}</button>
            <h2 class="form_info">{{info}}</h2>
        </form>
        {{/with}}
        </main>
        

        {{#with updates}}
        <div class="updates">
            <div class="description_title">{{title}}</div>
            </div>
            

            {{#each list}}
            <div class="my-3">
            <div class="updates_list">
                <strong>{{nickname}}</strong>
                <p>{{description}}</p>
            </div>
            </div>
            {{/each}}

            {{#with show_all}}
           <a href="{{url}}" class="list_link">{{text}}</a>
            {{/with}}
            {{/with}}



            {{#with expirience}}
             <div class="card_exp">
            <strong class="expirience_title">{{title}}</strong>
          
            
             </div>

              <div class="list">
               <div  class="expir_badge">{{badge}}</div>
                

                {{#each list}}
            
            <div class="list_card ">
            <div class="list_name">{{name}}</div>
            <p class="list_details">{{details}}</p>
            </div>
            
             
            {{/each}}
            </div>

            {{/with}}

           


                 <footer class="footer">
                     <div>
                    {{>logo}}
                    <span class="footer_copyright">Copyright 2023-2024</span>
                </div>

                {{#each footer}}
                <div>
                    <span class="footer_title">{{title}}</span>

                    {{> nav list=links isVertical=true}}
                </div>
                {{/each}}
                </footer>

 
